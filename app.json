{
  "expo": {
    "sdkVersion": "21.0.0",
    "name": "React Native Boilerplate",
    "description": "Simple Boilerplate",
    "slug": "react-native-boilerplate",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#00897B",
    "icon": "./asset/img/app-icon.png",
    "loading": {
      "icon": "./asset/img/app-icon.png",
      "hideExponentText": false
    },
    "packagerOpts": {
      "assetExts": ["ttf", "mp4"]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rizkiandrianto.boilerplate"
    },
    "android": {
      "package": "com.rizkiandrianto.boilerplate"
    }
  }
}